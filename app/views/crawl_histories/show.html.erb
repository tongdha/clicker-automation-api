<div id='crawl_history'>
  <h2><%= @crawl_history.startTime %></h2>
  <%= @crawl_history.endTime %>
</div>

<%= link_to "Edit Crawl History", [:edit, @crawler_config, @crawl_history] %>
<%= link_to "Delete Crawl History", [@crawler_config, @crawl_history], :method => :delete,
                     :confirm => "Are you sure you want to delete this Crawl History?" %>